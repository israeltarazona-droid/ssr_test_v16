{
  "name": "ssr-test-v16",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run ssr_test_v16:serve-ssr",
    "serve:ssr": "node dist/ssr_test_v16/server/main.js",
    "build:ssr": "ng build && ng run ssr_test_v16:server",
    "prerender": "ng run ssr_test_v16:prerender",
    "copy:ssr:server": "cpy ./dist/ssr_test_v16/server/**/* ./functions/dist/ssr_test_v16/server --parents",
    "copy:ssr:browser": "cpy ./dist/ssr_test_v16/browser/**/* ./functions/dist/ssr_test_v16/browser --parents",
    "build:firebase:ssr": "npm run build:ssr && npm run copy:ssr:server && npm run copy:ssr:browser"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/fire": "^16.0.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/platform-server": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "express": "^4.15.2",
    "firebase": "^12.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.0",
    "@nguniversal/builders": "^16.2.0",
    "@nguniversal/express-engine": "^16.2.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^16.11.7",
    "cpy-cli": "^6.0.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.1.3"
  }
}
